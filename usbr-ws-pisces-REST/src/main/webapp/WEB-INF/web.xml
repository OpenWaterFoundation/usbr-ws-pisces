    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <!-- Steve added below -->
  <servlet>
    <servlet-name>gov.usbr.ws.pisces.PiscesREST</servlet-name>
  </servlet>
  <servlet-mapping>
        <servlet-name>gov.usbr.ws.pisces.PiscesREST</servlet-name>
        <url-pattern>/v0/*</url-pattern>
  </servlet-mapping>
